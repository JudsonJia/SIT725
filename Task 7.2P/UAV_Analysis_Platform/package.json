  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --forceExit",
    "test:socket": "cross-env NODE_ENV=test jest tests/unit/socket --runInBand",
    "test:unit": "cross-env NODE_ENV=test jest tests/unit --runInBand",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest tests/e2e --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:original": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testTimeout": 15000,
    "forceExit": true,
    "detectOpenHandles": true,
    "maxWorkers": 1
  },
